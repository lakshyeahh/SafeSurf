{
  "manifest_version": 3,
  "name": "SafeSurf",
  "version": "1.0",
  "description": "A Chrome extension built with Vite, React, and TypeScript.",
  "permissions": [
    "activeTab",
    "contextMenus",
    "scripting"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "index.html"
  }
}
